<%- include ("../templates/encabezado")%>
<%- include ("../templates/menu")%>
<div class="container mt-5">
  <form action="/nuevomedico" method="post">
    <div class="card"></div>
      <div class="card-header">
        <h1>Nuevo medico</h1>
      </div>
      <div class="card-body">
        <input class="form-control mb-3" type="text" name="nombre" id="nombre" placeholder="Nombre" autofocus>
        <input class="form-control mb-3" type="text" name="apellidos" id="apellidos" placeholder="Apellidos" autofocus>
        <input class="form-control mb-3" type="text" name="usuario" id="usuario" placeholder="Usuario">
        <input class="form-control mb-3" type="password" name="password" id="password" placeholder="Password">
      </div>
      <div class="card-footer">
        <button class="form-control btn btn-primary" type="submit">Guardar</button>
      </div>
    </div>
  </form>
</div>
<%- include ("../templates/pie")%>
